{
  "name": "n8n-nodes-http-request-any-tool",
  "version": "0.9.4",
  "description": "a n8n http request tool node support always ok",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "washan",
    "email": "francismajere@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/washanhanzi/n8n-nodes-http-request-any-tool"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && esbuild nodes/HttpRequestAnyTool/HttpRequestAnyTool.node.ts --bundle --platform=node --target=node20 --outfile=dist/nodes/HttpRequestAnyTool/HttpRequestAnyTool.node.js --external:n8n-workflow --external:n8n-core && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/HttpRequestAnyTool/HttpRequestAnyTool.node.js"
    ]
  },
  "devDependencies": {
    "@mozilla/readability": "^0.6.0",
    "@types/html-to-text": "^9.0.4",
    "@types/lodash": "^4.17.21",
    "@types/ramda": "^0.31.1",
    "@typescript-eslint/parser": "~8.32.0",
    "cheerio": "^1.1.2",
    "esbuild": "^0.27.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "form-data": "^4.0.5",
    "gulp": "^5.0.0",
    "html-to-text": "^9.0.5",
    "linkedom": "^0.18.12",
    "lodash": "^4.17.21",
    "prettier": "^3.5.3",
    "ramda": "^0.32.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}